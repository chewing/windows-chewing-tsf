{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "新酷音詞庫管理程式",
  "version": "26.1.1",
  "identifier": "im.chewing.Chewing.Editor",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "title": "新酷音輸入法管理程式",
        "width": 930,
        "height": 600,
        "resizable": true,
        "visible": true,
        "maximizable": false,
        "windowEffects": { "effects": ["mica"] }
      },
      {
        "label": "about",
        "parent": "main",
        "url": "index.html#about",
        "title": "關於新酷音詞庫管理程式",
        "width": 400,
        "height": 450,
        "resizable": true,
        "visible": false,
        "closable": false,
        "maximizable": false,
        "minimizable": false,
        "windowEffects": { "effects": ["mica"] }
      }
    ],
    "security": {
      "csp": null
    }
  }
}
